<html>

    <head>
        <title>Sustain Exeter</title>
        <meta name=”Description” content=”Sustain is an Environmental magazine published by Exeter Students”>
        <meta name=”Keywords” content=”Sustain,Exonian,Exeter,Academy,PEA,Club,Magazine,Nature,Leaf,publish,newspaper,article”>
        <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link type="text/css" rel="stylesheet" href="/stylesheets/materialize.min.css" media="screen,projection" />
        <link type="text/css" rel="stylesheet" href="/stylesheets/custom.css" media="screen,projection" />
        <link type="text/css" rel="stylesheet" href="/stylesheets/mine.css" />
        <link type="text/css" rel="stylesheet" href="/stylesheets/opentip.css" />

        <meta name="author" content="Stuart Rucker">
        <!--Let browser know website is optimized for mobile-->
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <!--Import jQuery before materialize.js-->
        <script type="text/javascript" src="/javascript/ejsproduction.js"></script>
        <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
        <script type="text/javascript" src="/javascript/materialize.min.js"></script>
        <script type="text/javascript" src="/javascript/custom.js"></script>
        <script type="text/javascript" src="/javascript/opentip-jquery.min.js"></script>
        <script>
          new Opentip({ blackground: "1"})
        </script>
    </head>

    <body class="sky-gradient">
        <%- include nav.ejs %>


            <!-- header role="banner" -->
            <!-- header role="banner" -->
            <div class="tree-section" >
                <!-- header role="banner" -->
                <!-- header role="banner" -->
                <div class="hover-tip">
                    Hover to preview
                    <br>
                    <img style="" src="arrow.png"></img>
                </div>
                <div id="articleContainer">
                    <div class="list-hex-grid list-inline-block clearfix">
                        <%  var treeType = "leaf";
                          var zindex = 20;
                         for(var i = 0; i < Math.min(data.length, 10); i ++){
                            var article = data[i];
                            zindex --;
                           %>

                           <%- include hexagon-view.ejs %>

                            <%if(i == 2 || i == 6){%>
                              <br class="mq-mid mq-small" />
                              <br class="mq-wide" />
                            <%}%>
                        <%}%>




                        <br class="mq-mid mq-small" />
                        <br class="mq-wide" />
                        <div class="trunk-rapper outer">
                            <div class="trunk-rapper inner">

                              <%  var treeType = "trunk";

                               for(var i = 10; i < data.length; i ++){
                                  var article = data[i];
                                  zindex --;
                                 %>

                                 <%- include hexagon-view.ejs %>

                                  <%if(i == 11 || i == 14){%>
                                    <br class="mq-mid mq-small" />
                                    <br class="mq-wide" />
                                  <%}%>
                              <%}%>

                                <br class="mq-mid mq-small" />
                                <br class="mq-wide" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <%- include footer.ejs %>
